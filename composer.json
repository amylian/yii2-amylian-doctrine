{
  "name": "amylian/yii2-amylian-doctrine",
  "description": "ATTENTION EXPERIMENTAL! Docrine Meta Package",
  "type": "yii2-extension",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [{
      "name": "Andreas Prucha (Abexto - Helicon Software Development - Andreas Prucha / Amylian Project)",
      "email": "andreas.prucha@gmail.com",
      "homepage": "http://www.abexto.com",
      "role": "Developer"
    }    
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "require": {
    "amylian/yii2-amylian-doctrine-common": "dev-master",
    "amylian/yii2-amylian-doctrine-cache": "dev-master",
    "amylian/yii2-amylian-doctrine-dal": "dev-master"
  },
  "require-dev": {
    "amylian/yii2-amyian-phpunit": "*"
  },
  "autoload-dev": {
    "psr-4": {
      "abexto\\amylian\\yii\\doctrine\\tests\\": "tests/"
    }  
  },
}
